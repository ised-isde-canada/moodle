@Library('ised-cicd-lib') _

pipeline {
	agent none
   	
    options {
        disableConcurrentBuilds()
    }
   	
   	environment {
		// GLobal Vars
		OPENSHIFT_PROJECT = "learning-dev"
		IMAGE_NAME = "learning-moodle"
    }
    
    stages {
    	stage('deploy') {
	    	agent {
	    		label 'container-utils'
	    	}
	    	
			steps {
				script {
    				deploy("${OPENSHIFT_PROJECT}", "${IMAGE_NAME}")
    			}
    		}
    	}
    }
}